{
    "name": "SE Up-Down",
    "version": "0.7.0",
    "description": "Opens the up/down vote count for Stack Exchange sites",
    "permissions": [
        "tabs",
        "https://*/",
        "http://*/"
    ],
    "content_scripts": [{
        "matches": [
            "http://*/questions/*"
        ],
        "js": ["jquery1.7.js", "updownlib.js"],
        "css": ["style.css"],
        "run_at": "document_idle",
        "all_frames": false
    }]
}
